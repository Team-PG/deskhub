<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>DeskHub</title>
  <script src="https://kit.fontawesome.com/94c005aa00.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="/styles/reset.css">
  <link rel="stylesheet" href="/styles/base.css">
  <link rel="stylesheet" href="/styles/layout.css">
  <link rel="stylesheet" href="/styles/modules.css">
  <link rel="stylesheet" href="/styles/modules/jobs.css">

</head>
<body>
  <%- include("../../layout/header.ejs") %>

  <div id='wrapper'>
    <div id='center' class="center">
      <section>
        <form action="stocksShow" method="POST" id="searchStocks"> 
          <h2>Search Stock Quotes<br>(By Company Symbol)</h2>
          <input type="text"  name="searchStock" placeholder="Ex. AAPL for Apple">
          <button>Search</button>
          
        </form>
      </section>
      <ul>
        <h1>MOST ACTIVE STOCKS</h1>
          <% for (let i = 0; i < resultSuper.length; i++) { %>
            <li>
              <h1><%= resultSuper[i].ticker %></h1>
              <h1><%= resultSuper[i].changes %></h1>
              <h1><%= resultSuper[i].price %></h1>
              <h1><%= resultSuper[i].changesPercentage %></h1>
              <h1><%= resultSuper[i].companyName %></h1>
            </li>
          <% } %>
        </ul>
    </div>
  </div>
  <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="/login.js"></script>
</body>
</html>